graph = {
    'A': ['B', 'C'],
    'B': ['D', 'E'],
    'C': ['F'],
    'D': [],
    'E': [],
    'F': []
}

def bfs(start):
    visited = set()
    queue = deque([start])

    while queue:
        node = queue.popleft()
        if node not in visited:
            print(node, end=" ")
            visited.add(node)
            for neigh in graph[node]:
                queue.append(neigh)

# User input
start = input("Enter starting node (A-F): ")
print("BFS Traversal:")
bfs(start)
